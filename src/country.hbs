<h1 class="titleCountry">{{name}}</h1>
<div class="conuntryContainer_js">
    <div class="textCountry">
    <span class="headingContry">Capital: <span>{{capital}}</span></span>
    <span class="headingContry">Population: <span> {{population}}</span></span>
    <span class="headingContry">Language:</span>
    <ul>
        {{#each languages}}
        <li>{{this.name}}</li>
        {{/each}}
    </ul>
</div>
<div class="flagCountry">
    <img class="flag" src="{{flag}}" alt="flag">
</div>
</div>


